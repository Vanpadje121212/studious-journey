<%
if @script.nil? || @script.new_record?
  if @script.css?
    title t('scripts.new_title.css')
  else
    title t('scripts.new_title.js')
  end
else
  title t('scripts.update_title', script: @script.name)
end
%>

<%= render partial: 'script_versions/form', locals: {script_version: @script_version} %>
